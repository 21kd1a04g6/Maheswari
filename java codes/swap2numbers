class SwapVariables {
  static int a,b,c;
  static void swap(){
        a = a + b;
        b = a - b;
        a = a - b;
      
  }
    public static void main(String[] args)
    {
 a = 5;
 b = 10;
        System.out.println("Before swapping, a = " + a
                           + " and b = " + b);
        swap();
        
        System.out.println("After swapping, a = " + a
                           + " and b = " + b);
 }
}
